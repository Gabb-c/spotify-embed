{
  "name": "spotify-embed",
  "version": "1.0.0",
  "private": true,
  "description": "A spotify embed component",
  "repository": "git@github.com:Gabb-c/spotfy-embed.git",
  "license": "MIT",
  "author": {
    "name": "Gabb-c",
    "email": "gdgabrieldacunha71@gmail.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "prepare": "is-ci | husky install",
    "test:ci": "jest --ci --runInBand --bail --detectOpenHandles --errorOnDeprecated",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated --bail",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
